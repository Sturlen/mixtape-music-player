{
  "name": "bun-react-template",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "bun": "1.3.0"
  },
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun watch-routes\"  \"bun --hot src/index.tsx\"",
    "build": "bun generate-routes && bun typecheck && bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.tsx",
    "generate-routes": "tsr generate",
    "watch-routes": "tsr watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier .",
    "format:fix": "prettier . --write",
    "typecheck": "bun tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.3",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slider": "^1.3.6",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-pacer": "^0.17.4",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/react-router-devtools": "^1.133.1",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "elysia": "^1.4.11",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.546.0",
    "react": "^19",
    "react-dom": "^19",
    "react-swipeable": "^7.0.2",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tanstack/router-cli": "^1.132.51",
    "@types/bun": "^1.3.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript-eslint": "^8.46.3"
  }
}
