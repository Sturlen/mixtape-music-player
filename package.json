{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "bun": "1.3.2"
  },
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun watch-routes\"  \"bun --hot src/server/index.tsx\"",
    "build": "bun generate-routes && bun build ./src/client/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/server/index.tsx",
    "generate-routes": "tsr generate",
    "watch-routes": "tsr watch"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.6",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-router": "^1.134.15",
    "@tanstack/react-router-devtools": "^1.134.15",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "elysia": "^1.4.15",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.546.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-swipeable": "^7.0.2",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.134.15",
    "@types/bun": "^1.3.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2"
  }
}
